{
    "name": "ai-quiz-solver",
    "version": "1.0.0",
    "description": "AI-powered quiz solver application",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "node start.js",
        "server": "nodemon server.js",
        "client": "cd client && npm start",
        "install-all": "npm install && cd client && npm install",
        "build": "cd client && npm install && npm run build",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
    },
    "dependencies": {
        "axios": "^1.6.7",
        "cheerio": "^1.0.0-rc.12",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "firebase": "^10.8.0",
        "firebase-admin": "^12.0.0",
        "multer": "^1.4.5-lts.1",
        "pdf-lib": "^1.17.1",
        "pdf-parse": "^1.1.1",
        "sharp": "^0.33.5",
        "tesseract.js": "^5.1.1"
    },
    "devDependencies": {
        "concurrently": "^8.2.1",
        "nodemon": "^3.0.1",
        "googleapis": "^133.0.0",
        "google-auth-library": "^9.0.0"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}