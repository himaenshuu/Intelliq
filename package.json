{
    "name": "ai-quiz-solver",
    "version": "1.0.0",
    "description": "AI Quiz Solver Application",
    "main": "server/src/server.js",
    "scripts": {
        "start": "cd server && npm start",
        "build": "cd client && npm install && npm run build && cd ../server && npm install",
        "dev:server": "cd server && npm run dev",
        "dev:client": "cd client && npm start",
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\""
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.0",
        "axios": "^1.8.4",
        "cheerio": "^1.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.2",
        "firebase": "^10.8.0",
        "firebase-admin": "^12.0.0",
        "multer": "^1.4.5-lts.1",
        "pdf-lib": "^1.17.1",
        "pdf-parse": "^1.1.1",
        "sharp": "^0.33.5",
        "tesseract.js": "^5.1.1"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}